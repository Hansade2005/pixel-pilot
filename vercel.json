{
  "rewrites": [
    {
      "source": "/sites/:siteId",
      "destination": "/sites/:siteId/index.html"
    },
    {
      "source": "/sites/:siteId/",
      "destination": "/sites/:siteId/index.html"
    },
    {
      "source": "/sites/:siteId/(.*\\.js)",
      "destination": "/sites/:siteId/$1"
    },
    {
      "source": "/sites/:siteId/(.*\\.css)",
      "destination": "/sites/:siteId/$1"
    },
    {
      "source": "/sites/:siteId/(.*\\.png)",
      "destination": "/sites/:siteId/$1"
    },
    {
      "source": "/sites/:siteId/(.*\\.jpg)",
      "destination": "/sites/:siteId/$1"
    },
    {
      "source": "/sites/:siteId/(.*\\.jpeg)",
      "destination": "/sites/:siteId/$1"
    },
    {
      "source": "/sites/:siteId/(.*\\.gif)",
      "destination": "/sites/:siteId/$1"
    },
    {
      "source": "/sites/:siteId/(.*\\.webp)",
      "destination": "/sites/:siteId/$1"
    },
    {
      "source": "/sites/:siteId/(.*\\.svg)",
      "destination": "/sites/:siteId/$1"
    },
    {
      "source": "/sites/:siteId/(.*\\.ico)",
      "destination": "/sites/:siteId/$1"
    },
    {
      "source": "/sites/:siteId/(.*\\.woff)",
      "destination": "/sites/:siteId/$1"
    },
    {
      "source": "/sites/:siteId/(.*\\.woff2)",
      "destination": "/sites/:siteId/$1"
    },
    {
      "source": "/sites/:siteId/(.*\\.json)",
      "destination": "/sites/:siteId/$1"
    },
    {
      "source": "/sites/:siteId/(.*\\.txt)",
      "destination": "/sites/:siteId/$1"
    },
    {
      "source": "/sites/:siteId/(.*\\.xml)",
      "destination": "/sites/:siteId/$1"
    },
    {
      "source": "/sites/:siteId/(.*\\.map)",
      "destination": "/sites/:siteId/$1"
    },
    {
      "source": "/sites/:siteId/(.*)",
      "destination": "/sites/:siteId/index.html"
    }
  ],
  "headers": [
    {
      "source": "/sites/(.*)",
      "headers": [
        {
          "key": "X-Multi-Tenant",
          "value": "true"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/cron/send-notifications",
      "schedule": "0 9 * * *"
    }
  ]
}
